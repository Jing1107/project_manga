
<h1>All Artists</h1>

<% @artists.each do |artist| %>

  <div class="index-artist-grid">
    <%= link_to image_tag(artist.image, :alt => artist.name, :class => 'artist_thumbnail'), artist if artist.image.present? %>

      <ul>
        <li><%= link_to artist.name, artist  %></li>
      </ul>
  </div>
<% end %>
